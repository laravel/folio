{
    "name": "laravel/folio",
    "description": "Page based routing for Laravel.",
    "keywords": ["laravel", "routing"],
    "homepage": "https://github.com/laravel/folio",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/folio/issues",
        "source": "https://github.com/laravel/folio"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/container": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/pipeline": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0",
        "spatie/once": "^3.1",
        "symfony/finder": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Folio\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Folio\\FolioServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
